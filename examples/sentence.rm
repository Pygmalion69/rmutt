#include "w710.rm"
#include "util.rm"

package sentence;

line: s "\n";

s: snp > util.xcap " " w710.verb_trans_past " " onp ".";
snp: np | spn;
spn: "I"|"you"|"we"|"he"|"she"|"they";
onp: np | opn;
opn: "me"|"you"|"us"|"her"|"him"|"them";
np: (art " " w710.adv) > util.xan " " w710.adj " " w710.noun_singular |
    (art " " w710.adj) > util.xan " " w710.noun_singular |
    (art " " w710.noun_singular) > util.xan;
art: "a"|"the";

